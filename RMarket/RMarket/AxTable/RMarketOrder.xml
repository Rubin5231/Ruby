		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static RMarketOrder find(RMarketOrderId  _orderId,
                                    boolean         _forUpdate = false)
    {
        RMarketOrder orders;
        ;
        if (_orderId)
        {
            orders.selectForUpdate(_forUpdate);
            
            select firstonly orders
                where orders.OrderId  == _orderId;
        }
        
        return orders;
    }

]]></Source>
