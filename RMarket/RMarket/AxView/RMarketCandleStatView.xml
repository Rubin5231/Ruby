				<Source><![CDATA[
    public static server str MissedCandles()
    {
        str expression = strfmt('((max(%1) - min(%1)) / %2) + 1 - count(%3)',
            SysComputedColumn::returnField(tablestr(RMarketCandleStatView), identifierstr(RMarketCandleTable), fieldstr(RMarketCandleTable, Timestamp)),
            SysComputedColumn::returnField(tablestr(RMarketCandleStatView), identifierstr(RMarketDim), fieldstr(RMarketDim, Seconds)),
            SysComputedColumn::returnField(tablestr(RMarketCandleStatView), identifierstr(RMarketCandleTable), fieldstr(RMarketCandleTable, RecId))
            );

        return expression;
    }

]]></Source>
