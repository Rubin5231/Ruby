						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            super();

            if (RMarketSubAccount_DS.leaveRecord())
            {
                RMarketSubAccount_DS.create();
            }
        }

]]></Source>
