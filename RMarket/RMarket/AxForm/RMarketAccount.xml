			<Control>
				<Name>SubaccountDeleteButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            RMarketSubAccount  subAccount;

            super();
    
            subAccount = RMarketSubAccount_DS.cursor();
            if (subAccount.RecId && subAccount.validateDelete())
            {
                try
                {
                    subAccount.delete();
                }
                catch
                {
                    //continue with other records
                }
            }
            RMarketSubAccount_DS.research();
        }

]]></Source>
