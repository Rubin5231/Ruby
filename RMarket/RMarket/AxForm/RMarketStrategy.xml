				<Name>RMarketConditionTable</Name>
				<Methods>
					<Method>
						<Name>create</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        /// <param name = "_append"></param>
        public void create(boolean _append = false)
        {
            RMarketConditionTable condition;

            super(_append);

            select maxof(Num) from condition
                where condition.RefRecId == RMarketConditionTable.RefRecId;
            
            RMarketConditionTable record = this.cursor();
            //record.RefRecId = RMarketStrategyAction.RecId;
            record.Num   = condition.Num + 1;
            record.LeftInterval = RMarketStrategy.Interval;
            record.RightInterval = RMarketStrategy.Interval;
        }

]]></Source>
