]]></Declaration>
		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static RMarketSubAccount find(RMarketSubAccountId  _subAccountId,
                                         boolean              _forUpdate = false)
    {
        RMarketSubAccount subAccount;
        ;
        if (_subAccountId)
        {
            subAccount.selectForUpdate(_forUpdate);
            
            select firstonly subAccount
                where subAccount.SubAccountId  == _subAccountId;
        }
        
        return subAccount;
    }

]]></Source>
			</Method>
			<Method>
				<Name>initValue</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void initValue()
    {
        RMarketSubAccountId subAccountId;
        NumberSequenceReference numberSequenceReference;

        super();

        numberSequenceReference = NumberSeqReference::findReference(extendedTypeNum(RMarketSubAccountId));
        if (numberSequenceReference)
        {
            this.SubAccountId = NumberSeq::newGetNum(numberSequenceReference).num();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initFromAccount</Name>
				<Source><![CDATA[
    public void initFromAccount(RMarketAccount _account)
    {
        this.AccountId = _account.AccountId;
    }

]]></Source>
			</Method>
			<Method>
				<Name>RMarketAccount</Name>
				<Source><![CDATA[
    public RMarketAccount RMarketAccount(boolean _forUpdate = false)
    {
        return RMarketAccount::find(this.AccountId, _forUpdate);
    }

]]></Source>
			</Method>
