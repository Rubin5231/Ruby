		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static RMarketPosition find(RMarketPositionId  _positionId,
                                       boolean            _forUpdate = false)
    {
        RMarketPosition position;
        ;
        if (_positionId)
        {
            position.selectForUpdate(_forUpdate);
            
            select firstonly position
                where position.PositionId  == _positionId;
        }
        
        return position;
    }

]]></Source>
