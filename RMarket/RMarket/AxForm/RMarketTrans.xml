    /// <summary>
    ///
    /// </summary>
    public void init()
    {
        super();

        if (element.args().record())
        {
            switch (element.args().dataset())
            {
                case tableNum(RMarketAccount):
                case tableNum(RMarketSubAccount):
                    rangeRecord = element.args().record();
                    break;
            }
        }
    }

