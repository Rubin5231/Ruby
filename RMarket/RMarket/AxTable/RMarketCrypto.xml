				<Source><![CDATA[
    public static RMarketCrypto find(RMarketCryptoCode  _cryptoCode,
                                     boolean            _forUpdate = false)
    {
        RMarketCrypto crypto;
        ;
        if (_cryptoCode)
        {
            crypto.selectForUpdate(_forUpdate);
            
            select firstonly crypto
                where crypto.CryptoCode  == _cryptoCode;
        }
        
        return crypto;
    }

]]></Source>
