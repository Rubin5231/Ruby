		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static RMarketStrategy find(RMarketStrategyId  _strategyId,
                                       boolean            _forUpdate = false)
    {
        RMarketStrategy strategy;
        ;
        if (_strategyId)
        {
            strategy.selectForUpdate(_forUpdate);
            
            select firstonly strategy
                where strategy.StrategyId  == _strategyId;
        }
        
        return strategy;
    }

]]></Source>
			</Method>
			<Method>
				<Name>initValue</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void initValue()
    {
        RMarketStrategyId strategyId;
        NumberSequenceReference numberSequenceReference;
        
        super();

        numberSequenceReference = NumberSeqReference::findReference(extendedTypeNum(RMarketStrategyId));
        if (numberSequenceReference)
        {
            strategyId = NumberSeq::newGetNum(numberSequenceReference).num();
            this.StrategyId = strategyId;
        }

        this.Interval = RMarketCandleInterval::None;
    }

]]></Source>
			</Method>
