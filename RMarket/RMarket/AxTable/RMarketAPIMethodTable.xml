				<Name>find</Name>
				<Source><![CDATA[
    public static RMarketAPIMethodTable find(RMarketId            _marketId,
                                             RMarketAPIMethodEnum _methodEnum,
                                             boolean              _forUpdate = false)
    {
        RMarketAPIMethodTable marketAPIMethodTable;
        ;
        if (_marketId)
        {
            marketAPIMethodTable.selectForUpdate(_forUpdate);
            
            select firstonly marketAPIMethodTable
                where marketAPIMethodTable.MarketId   == _marketId
                   && marketAPIMethodTable.MethodEnum == _methodEnum;
        }
        
        return marketAPIMethodTable;
    }

]]></Source>
			</Method>
			<Method>
				<Name>RMarketTable</Name>
				<Source><![CDATA[
    public RMarketTable RMarketTable()
    {
        return RMarketTable::find(this.MarketId);
    }

]]></Source>
			</Method>
			<Method>
				<Name>url</Name>
				<Source><![CDATA[
    public RMarketAPIMethodURL url()
    {
        RMarketAPIMethodURL ret = this.RMarketTable().ApiURL;
        ret += (subStr(this.MethodURL, 1, 1) != '/') ? '/' : '';
        ret += this.MethodURL;
        return ret;
    }

]]></Source>
