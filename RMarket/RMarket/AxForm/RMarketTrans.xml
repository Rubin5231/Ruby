						<Name>executeQuery</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void executeQuery()
        {
            QueryBuildDataSource  qbds = this.query().dataSourceTable(tablenum(RMarketTrans));

            switch (rangeRecord.TableId)
            {
                case tableNum(RMarketAccount):
                    RMarketAccount account = rangeRecord;
                    qbds.addRange(fieldNum(RMarketTrans, AccountId)).value(SysQuery::value(account.AccountId));
                    break;

                case tableNum(RMarketSubAccount):
                    RMarketSubAccount subAccount = rangeRecord;
                    qbds.addRange(fieldNum(RMarketTrans, SubAccountId)).value(SysQuery::value(subAccount.SubAccountId));
                    break;
            }

            super();
        }

]]></Source>
