						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        /// <param name = "_append"></param>
        public void create(boolean _append = false)
        {
            RMarketStrategyAction action;

            super(_append);

            select maxof(Num) from action
                where action.StrategyId == RMarketStrategy.StrategyId;

            RMarketStrategyAction record = this.cursor();
            //record.StrategyId = RMarketStrategy.StrategyId;
            record.Num   = action.Num + 1;
        }

]]></Source>
