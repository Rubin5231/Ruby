						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        /// <returns></returns>
        public int active()
        {
            int ret;
    
            ret = super();

            AccountTestTabPage.visible(RMarketAccount.AccountType == RMarketAccountType::Test);
            RMarketAutoTrade.enabled(RMarketAccount.AccountType == RMarketAccountType::Test);
    
            return ret;
        }

