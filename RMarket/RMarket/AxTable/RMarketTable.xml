				<Source><![CDATA[
    public static RMarketTable find(RMarketId   _marketId,
                                    boolean     _forUpdate = false)
    {
        RMarketTable marketTable;
        ;
        if (_marketId)
        {
            marketTable.selectForUpdate(_forUpdate);
            
            select firstonly marketTable
                where marketTable.MarketId  == _marketId;
        }
        
        return marketTable;
    }

]]></Source>
