		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static RMarketIntervals find(RMarketId             _marketId,
                                        RMarketCandleInterval _interval,
                                        boolean               _forUpdate = false)
    {
        RMarketIntervals RMarketIntervals;
        ;
        if (_marketId)
        {
            RMarketIntervals.selectForUpdate(_forUpdate);
            
            select firstonly RMarketIntervals
            where RMarketIntervals.MarketId    == _marketId
               && RMarketIntervals.Interval    == _interval;
        }
        
        return RMarketIntervals;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
