		<Methods>
			<Method>
				<Name>findRecId</Name>
				<Source><![CDATA[
    public static RMarketEventLog findRecId(RecId    _recId,
                                            boolean  _forUpdate = false)
    {
        RMarketEventLog log;
        ;
        if (_recId)
        {
            log.selectForUpdate(_forUpdate);
            
            select firstonly log
                where log.RecId  == _recId;
        }
        
        return log;
    }

]]></Source>
			</Method>
			<Method>
				<Name>putRecord</Name>
				<Source><![CDATA[
    /// <summary>
    /// Usage:
    /// RMarketEventLog::putRecord(AccountId, SubaccountId, RMarketLogLevel::Info, RMarketEventType::ErrorOccurred, 
    ///     "");
    /// </summary>
    public static void putRecord(
        RMarketAccountId    _accountId    = "",
        RMarketSubAccountId _subAccountId = "",
        RMarketLogLevel     _logLevel     = RMarketLogLevel::None,
        RMarketEventType    _eventType    = RMarketEventType::ErrorOccurred,
        Description255      _description  = "")
    {
        RMarketEventLog log;
        ttsbegin;
        log.clear();
        log.initValue();
        log.AccountId = _accountId;
        log.SubAccountId = _subAccountId;
        log.LogLevel = _logLevel;
        log.EventType = _eventType;
        log.Description = _description;
        if (log.validateWrite())
            log.insert();
        ttscommit;
    }

]]></Source>
			</Method>
