				<Name>find</Name>
				<Source><![CDATA[
    public static RMarketSymbolTable find(RMarketSymbolId   _symbolId,
                                          boolean           _forUpdate = false)
    {
        RMarketSymbolTable marketSymbolTable;
        ;
        if (_symbolId)
        {
            marketSymbolTable.selectForUpdate(_forUpdate);
            
            select firstonly marketSymbolTable
                where marketSymbolTable.SymbolId  == _symbolId;
        }
        
        return marketSymbolTable;
    }

]]></Source>
